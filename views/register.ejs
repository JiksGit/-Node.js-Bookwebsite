
    <h1>회원 가입</h1>

    <form action="/register_process" method="post">
        <div class="row mb-3">
          <label for="inputEmail3" class="col-sm-2  col-form-label">로그인 ID *</label>
          <div class="col-sm-5">
            <input type="id" class="form-control" placeholder="ID" name="loginid" maxlength="20">
          </div>
        </div>
        <div class="row mb-3">
          <label for="inputPassword3" class="col-sm-2 col-form-label">Password *</label>
          <div class="col-sm-5">
            <input type="password" class="form-control" id="inputPassword3" name="password" maxlength="20">
          </div>
        </div>
        <div class="row mb-3">
            <label for="inputEmail3" class="col-sm-2  col-form-label">이름 *</label>
            <div class="col-sm-5">
              <input type="id" class="form-control" placeholder="이름" name="name" >
            </div>
        </div>
        <div class="row mb-3">
            <label for="inputEmail3" class="col-sm-2  col-form-label">주소</label>
            <div class="col-sm-5">
              <input type="id" class="form-control" placeholder="25자 이하" name="address">
            </div>
        </div>
        <div class="row mb-3">
            <label for="inputEmail3" class="col-sm-2  col-form-label">전화번호 *</label>
            <div class="col-sm-5">
              <input type="id" class="form-control" placeholder="000-0000-0000" name="tel">
            </div>
        </div>
        <div class="row mb-3">
            <label for="inputEmail3" class="col-sm-2  col-form-label">생일 *</label>
            <div class="col-sm-5">
              <input type="id" class="form-control" placeholder="00000000" name="birth">
            </div>
        </div>
        <div class="row mb-3">
            <label for="inputEmail3" class="col-sm-2  col-form-label">class *</label>
            <div class="col-sm-5">
              <input type="id" class="form-control" placeholder="class" name="class2">
            </div>
        </div>
        <div class="row mb-3">
            <label for="inputEmail3" class="col-sm-2  col-form-label">grade *</label>
            <div class="col-sm-5">
              <input type="id" class="form-control" placeholder="grade" name="grade">
            </div>
        </div>
              
        <button type="submit" class="btn btn-primary">Sign in</button>
    </form>