<h1>네임 카드</h1>  
<table class="table table-sm table-bordered" style="width : 600px">  
    <%  
        var i = 0;  
        while(i < results.length){  
    %>  
        <tr><td class="td td-sm"><%=results[i].id%></td>  
            <td><%=results[i].name%></td>  
            <td><%=results[i].gender%></td>  
            <td><%=results[i].age%></td>  
            <td><%=results[i].address%></td>    
            <td><a href="/namecard/update/<%=results[i].id%>">update</a></td> 
            <td><a href="/namecard/delete_process/<%=results[i].id%>">delete</a></td>  
        </tr>
    <%
        i++;
    }
    %>
    <a href="/namecard/create">create</a>
</table>